{
	"name": "Trival Colors Dark",
	"settings": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "token",
			"settings": {
				"foreground": "#c5c8ca"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#c5c8ca"
			}
		},
		{
			"name": "comments",
			"scope": "comment",
			"settings": {
				"foreground": "#60606f"
			}
		},
		{
			"scope": [
				"constant",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#dc79ec"
			}
		},
		{
			"scope": "numeric",
			"settings": {
				"foreground": "#dc79ec"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"punctuation.definition.string.tag"
			],
			"settings": {
				"foreground": "#a0d970"
			}
		},
		{
			"name": "keywords",
			"scope": [
				"keyword",
				"storage",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#60d0d0"
			}
		},
		{
			"name": "types",
			"scope": [
				"type",
				"type.entity.name",
				"function.type.entity.name",
				"function.support.type",
				"support.class",
				"support.math",
				"constant.support.math",
				"support.dom",
				"punctuation.definition.type.parameters"
			],
			"settings": {
				"foreground": "#90b6eE"
			}
		},
		{
			"name": "variables",
			"scope": [
				"variable.parameter",
				"variable.alias",
				"variable.var-single-variable",
				"variable.object-binding-pattern",
				"variable.object-binding-pattern-variable",
				"variable.array-binding-pattern",
				"variable.array-binding-pattern-variable",
				"object-literal.member.key",
				"support.property-name"
			],
			"settings": {
				"foreground": "#e0b8f7"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#ecDcA2"
			}
		},
		{
			"name": "noise",
			"scope": [
				"punctuation.separator.key-value",
				"punctuation.definition",
				"brace"
			],
			"settings": {
				"foreground": "#82829C"
			}
		},
		{
			"name": "big noise",
			"scope": [
				"terminator",
				"comma",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#60606f"
			}
		},
		{
			"name": "special punctuation",
			"scope": [
				"brace.square",
				"punctuation.binding-pattern"
			],
			"settings": {
				"foreground": "#90b6eE"
			}
		},
		




		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "entity.name.selector",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "entity.other.attribute-name.css",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
