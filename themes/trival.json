{
  "$schema": "vscode://schemas/color-theme",
  "name": "Trival Colors Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#1F2528",
    "editor.foreground": "#cccfcb",
    "activityBar.background": "#171717",
    "sideBar.background": "#171717",
    "sideBar.foreground": "#bbbbbb",
    "terminal.background": "#171717"
  },
  "semanticTokenColors": {
    "function.declaration": "#e0cb93",
    "method.declaration": "#e0cb93",
    "macro": "#eeaf96"
  },
  "tokenColors": [
    {
      "name": "white - override template string coloring",
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#cccfcb"
      }
    },
    {
      "name": "gray",
      "scope": [
        "comment",
        "punctuation.terminator",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#60606f"
      }
    },
    {
      "name": "turquoise",
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.control",
        "punctuation.definition.keyword",

        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#53cfcf"
      }
    },
    {
      "name": "green",
      "scope": [
        "string",
        "string.tag",
        "string.value",
        "punctuation.definition.string"
      ],

      "settings": {
        "foreground": "#9dd076"
      }
    },
    {
      "name": "purple",
      "scope": [
        "constant",
        "support.constant",
        "entity.name.import",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#da7cd7"
      }
    },
    {
      "name": "purple light",
      "scope": [
        "variable.parameter",
        "variable.other.declaration",
        "meta.definition.variable",
        "meta.object-literal.key",
        "meta.interface variable",
        "meta.type variable",
        "support.type.property-name",

        "meta.interface meta.definition entity.name.function",
        "meta.field meta.definition entity.name.function",
        "variable.language.self"
      ],
      "settings": {
        "foreground": "#d5b8f7"
      }
    },

    {
      "name": "blue",
      "scope": [
        "meta.type",
        "meta.interface",
        "support.type",
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "entity.other.inherited-class",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#83a8ef"
      }
    },
    {
      "name": "blue-light",
      "scope": [
        "entity.name.function",
        "support.function",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#a6cfe9"
      }
    },
    {
      "name": "light orange",
      "scope": [
        "meta.definition entity.name.function",
        "meta.function.definition entity.name.function"
      ],
      "settings": {
        "foreground": "#e0cb93"
      }
    },
    {
      "name": "orange",
      "scope": [
        "string.regexp",
        "constant.regexp",
        "entity.other.attribute-name",
        "entity.name.package",
        "entity.name.class.declaration",
        "meta.class meta.field.declaration variable",
        "punctuation.definition.template-expression",
        "meta.macro.rust",
        "meta.attribute.rust",
        "entity.name.function.macro.rust",
        "punctuation.definition.attribute.rust",
        "punctuation.brackets.attribute.rust"
        // "entity.other.attribute-name punctuation.definition.entity",
      ],
      "settings": {
        "foreground": "#eeaf96"
      }
    },
    {
      "name": "red",
      "scope": ["entity.name.tag", "entity.name.tag support.class"],
      "settings": {
        "foreground": "#f980aa"
      }
    },
    {
      "name": "dark-purple",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#9a6fc8"
      }
    },
    {
      "name": "bold",
      "scope": [
        "meta.definition entity.name.function",
        "meta.function.definition entity.name.function"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": [
        "entity.other.attribute-name",
        "entity.name.tag",
        "entity.name.function.macro.rust"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "noise",
      "scope": [
        "meta.brace",
        "meta.bracket",
        "punctuation.brackets",
        "punctuation.section",
        "punctuation.definition",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.colon",
        "punctuation.operation",
        "punctuation.semi",
        "punctuation.comma"
      ],
      "settings": {
        "foreground": "#8393b6"
      }
    },

    // === some old colors ===

    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },

    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    }
  ]
}
