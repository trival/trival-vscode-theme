{
  "$schema": "vscode://schemas/color-theme",
  "name": "Trival Colors Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#1F2528",
    "editor.foreground": "#cccfcb",
    "activityBar.background": "#171717",
    "sideBar.background": "#171717",
    "sideBar.foreground": "#bbbbbb",
    "terminal.background": "#171717"
  },
  "semanticTokenColors": {
    "type": "#83a8ef",
    "variable": { "underline": true },
    "variable:rust": { "underline": false },
    "variable.readonly": { "underline": false },
    "variable.mutable": { "underline": true },
    "variable.declaration": { "foreground": "#d5b8f7" },
    "variable.readonly.declaration": { "foreground": "#d5b8f7"},
    "variable.defaultLibrary": { "foreground": "#da7cd7", "underline": false },
    "property.declaration": { "foreground": "#d5b8f7" },
    "class.defaultLibrary": { "foreground": "#da7cd7" },
    "function": { "foreground": "#a4cef5" },
    "method": { "foreground": "#a4cef5" },
    "function.declaration": { "foreground": "#e0cb93" },
    "method.declaration": { "foreground": "#e0cb93" },
    "function.definition": { "foreground": "#e0cb93" },
    "method.definition": { "foreground": "#e0cb93" },
    "parameter": "#a0dcd0",
    "macro": "#eeaf96",
    "enumMember": "#da7cd7"
  },
  "tokenColors": [
    {
      "name": "white - override template string coloring",
      "settings": {
        "foreground": "#cccfcb"
      },
      "scope": [
        "string variable.other"
      ]
    },

    {
      "name": "gray",
      "settings": {
        "foreground": "#60606f"
      },
      "scope": [
        "comment",
        "punctuation.terminator",
        "punctuation.definition.comment"
      ]
    },

    {
      "name": "turquoise",
      "settings": {
        "foreground": "#53cfcf"
      },
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.control",
        "punctuation.definition.keyword",

        "storage",
        "storage.type",
        "storage.modifier"
      ]
    },

    {
      "name": "green",
      "scope": [
        "string",
        "string.tag",
        "string.value",
        "punctuation.definition.string"
      ],

      "settings": {
        "foreground": "#9dd076"
      }
    },

    {
      "name": "purple",
      "settings": {
        "foreground": "#da7cd7"
      },
      "scope": ["constant", "support.constant", "entity.name.import"]
    },

    {
      "name": "purple light",
      "settings": {
        "foreground": "#d5b8f7"
      },
      "scope": [
        "meta.definition.variable",
        "variable.other.declaration",

        "support.type.property-name",
        "meta.object-literal.key",
        "meta.interface variable",
        "meta.type variable",
        "support.type.property-name",

        "meta.interface meta.definition entity.name.function",
        "meta.field meta.definition entity.name.function",
      ]
    },

    {
      "name": "turquoise light",
      "settings": {
        "foreground": "#a0dcd0"
      },
      "scope": [
        "variable.parameter",
        "variable.language.self",
        "variable.language.this"
      ]
    },

    {
      "name": "blue",
      "settings": {
        "foreground": "#83a8ef"
      },
      "scope": [
        "meta.type",
        "meta.interface",
        "support.type",
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "entity.other.inherited-class",
        "entity.name.module"
      ]
    },

    {
      "name": "blue-light",
      "settings": {
        "foreground": "#a4cef5"
      },
      "scope": [
        "entity.name.function",
        "support.function",
        "punctuation.decorator"
      ]
    },

    {
      "name": "light orange",
      "scope": [
        "entity.name.function.declaration",
        "meta.definition entity.name.function",
        "meta.function.definition entity.name.function"
      ],
      "settings": {
        "foreground": "#e0cb93"
      }
    },

    {
      "name": "orange",
      "settings": {
        "foreground": "#eeaf96"
      },
      "scope": [
        "string.regexp",
        "constant.regexp",
        "entity.other.attribute-name",
        "entity.name.package",
        "entity.name.class.declaration",
        "meta.class meta.field.declaration variable",
        "punctuation.definition.template-expression",
        "meta.macro.rust",
        "meta.attribute.rust",
        "entity.name.function.macro.rust",
        "punctuation.definition.attribute.rust",
        "punctuation.brackets.attribute.rust"
        // "entity.other.attribute-name punctuation.definition.entity",
      ]
    },

    {
      "name": "red",
      "settings": {
        "foreground": "#f980aa"
      },
      "scope": ["entity.name.tag", "entity.name.tag support.class"]
    },

    {
      "name": "dark-purple",
      "settings": {
        "foreground": "#9a6fc8"
      },
      "scope": ["punctuation.definition.tag"]
    },

    {
      "name": "bold",
      "settings": {
        "fontStyle": "bold"
      },
      "scope": [
        "entity.name.function.declaration",
        "meta.definition entity.name.function",
        "meta.function.definition entity.name.function"
      ]
    },

    {
      "name": "italic",
      "settings": {
        "fontStyle": "italic"
      },
      "scope": [
        "entity.other.attribute-name",
        "entity.name.tag",
        "entity.name.function.macro.rust"
      ]
    },

    {
      "name": "noise",
      "settings": {
        "foreground": "#8393b6"
      },
      "scope": [
        "meta.brace",
        "meta.bracket",
        "punctuation.brackets",
        "punctuation.section",
        "punctuation.definition",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.colon",
        "punctuation.operation",
        "punctuation.semi",
        "punctuation.comma"
      ]
    },

    // === some old colors ===

    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },

    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    }
  ]
}
