{
  "$schema": "vscode://schemas/color-theme",
  "name": "Trival Colors Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#d7dde0",
    "editor.foreground": "#31332f",
    "editor.lineHighlightBorder": "#c2ccd088",
    "activityBar.background": "#eaeaea",
    "activityBar.foreground": "#31332f",
    "tab.inactiveBackground": "#eaeaea",
    "tab.activeBackground": "#d7dde0",
    "tab.border": "#bbb",
    "editorGroupHeader.tabsBackground": "#eaeaea",
    "editorGroupHeader.noTabsBackground": "#eaeaea",
    "sideBarSectionHeader.background": "#d7dde0",
    "sideBar.background": "#eaeaea",
    "sideBar.foreground": "#4c4c4c",
    "terminal.background": "#eaeaea",

    "focusBorder": "#bbb",
    "panel.border": "#bbb",
    "sideBar.border": "#bbb",
    "editorGroup.border": "#bbb",
    "statusBar.border": "#bbb",

    "panel.background": "#eaeaea",

    "statusBar.background": "#eaeaea",
    "statusBar.foreground": "#31332f",

    "editor.selectionBackground": "#b6d2e0",
    "editor.selectionHighlightBackground": "#b6d2e0cc",
    "editor.wordHighlightBackground": "#b6d2e0cc",

    "scrollbar.shadow": "#00000022",
    "widget.shadow": "#00000022",
    "scrollbarSlider.background": "#fff9f280",
    "scrollbarSlider.hoverBackground": "#3f3f3f10",

    "list.hoverBackground": "#d7dde0",
    "list.activeSelectionBackground": "#b6d2e0",
    "list.inactiveSelectionBackground": "#b6d2e0",
    "editorLineNumber.foreground": "#48587c",
    "editorLineNumber.activeForeground": "#31332f",

    "menu.background": "#dfe2e4",
    "dropdown.background": "#dfe2e4",
    "dropdown.listBackground": "#dfe2e4",
    "dropdown.border": "#bbb",
    "quickInput.background": "#dfe2e4",
    "editorWidget.background": "#dfe2e4",
    "editorWidget.border": "#bbb",
    "notifications.background": "#dfe2e4",
    "notificationCenterHeader.background": "#dfe2e4",

    "input.background": "#d7dde0",
    "editorIndentGuide.background1": "#c2ccd0"
  },
  "semanticTokenColors": {
    "type": "#10347c",
    "variable": { "underline": true },
    "variable:rust": { "underline": false },
    "variable:glsl": { "underline": false },
    "variable:fsharp": { "underline": false },
    "variable.readonly": { "underline": false },
    "variable.mutable": { "underline": true },
    "variable.declaration": { "foreground": "#330c4c" },
    "variable.readonly.declaration": { "foreground": "#330c4c" },
    "variable.constant.declaration": { "foreground": "#832580" },
    "variable.defaultLibrary": { "foreground": "#832580", "underline": false },
    "class.defaultLibrary": { "foreground": "#832580" },
    "enumMember": { "foreground": "#832580" },
    "property.declaration": { "foreground": "#330c4c" },
    "function": { "foreground": "#0d3458" },
    "function.declaration": { "foreground": "#6c571e" },
    "function.definition": { "foreground": "#6c571e", "bold": true },
    "method": { "foreground": "#0d3458" },
    "method.declaration": { "foreground": "#6c571e" },
    "method.declaration:rust": { "foreground": "#6c571e", "bold": true },
    "method.definition": { "foreground": "#6c571e", "bold": true },
    "parameter": { "foreground": "#276241" },
    "macro": { "foreground": "#692a11" }
  },
  "tokenColors": [
    {
      "name": "white - override template string coloring",
      "settings": {
        "foreground": "#31332f"
      },
      "scope": ["string variable.other", "support.constant.property"]
    },

    {
      "name": "gray",
      "settings": {
        "foreground": "#90909f"
      },
      "scope": [
        "comment",
        "punctuation.terminator",
        "punctuation.definition.comment"
      ]
    },

    {
      "name": "turquoise",
      "settings": {
        "foreground": "#30abab"
      },
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.control",
        "punctuation.definition.keyword",
        "punctuation.accessor.optional",

        "storage",
        "storage.type",
        "storage.modifier"
      ]
    },

    {
      "name": "green",
      "scope": [
        "string",
        "string.tag",
        "string.value",
        "punctuation.definition.string"
      ],

      "settings": {
        "foreground": "#428510"
      }
    },

    {
      "name": "purple",
      "settings": {
        "foreground": "#832580"
      },
      "scope": ["constant", "support.constant", "entity.name.import"]
    },

    {
      "name": "purple light",
      "settings": {
        "foreground": "#330c4c"
      },
      "scope": [
        "meta.definition.variable",
        "variable.other.declaration",
        "variable.language.self",
        "variable.language.this",

        "support.type.property-name",
        "meta.object-literal.key",
        "meta.interface variable",
        "meta.type variable",
        "support.type.property-name",

        "meta.interface meta.definition entity.name.function",
        "meta.field meta.definition entity.name.function",

        "binding.fsharp variable.fsharp",
        "markup.inline.raw.string.markdown"
      ]
    },

    {
      "name": "turquoise light",
      "settings": {
        "foreground": "#276241"
      },
      "scope": ["variable.parameter"]
    },

    {
      "name": "blue",
      "settings": {
        "foreground": "#10347c"
      },
      "scope": [
        "meta.type",
        "meta.interface",
        "support.type",
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "entity.other.inherited-class",
        "entity.name.module",
        "markup.underline.link"
      ]
    },

    {
      "name": "blue-light",
      "settings": {
        "foreground": "#0d3458"
      },
      "scope": [
        "entity.name.function",
        "support.function",
        "punctuation.decorator"
      ]
    },

    {
      "name": "light orange",
      "scope": [
        "entity.name.function.declaration",
        "meta.definition entity.name.function",
        "meta.function.definition entity.name.function"
      ],
      "settings": {
        "foreground": "#6c571e"
      }
    },

    {
      "name": "orange",
      "settings": {
        "foreground": "#692a11"
      },
      "scope": [
        "string.regexp",
        "constant.regexp",
        "entity.other.attribute-name",
        "entity.name.package",
        "entity.name.class.declaration",
        "meta.class meta.field.declaration variable",
        "punctuation.definition.template-expression",
        "meta.macro.rust",
        "meta.attribute.rust",
        "entity.name.function.macro.rust",
        "punctuation.definition.attribute.rust",
        "punctuation.brackets.attribute.rust"
        // "entity.other.attribute-name punctuation.definition.entity",
      ]
    },

    {
      "name": "red",
      "settings": {
        "foreground": "#7f052f"
      },
      "scope": [
        "entity.name.tag",
        "entity.name.tag support.class",
        "markup.heading.markdown"
      ]
    },

    {
      "name": "dark-purple",
      "settings": {
        "foreground": "#623690"
      },
      "scope": ["punctuation.definition.tag"]
    },

    {
      "name": "bold",
      "settings": {
        "fontStyle": "bold"
      },
      "scope": [
        "entity.name.function.declaration",
        "meta.definition entity.name.function",
        "meta.function.definition entity.name.function",

        "markup.heading.markdown",
        "markup.bold",
        "strong"
      ]
    },

    {
      "name": "italic",
      "settings": {
        "fontStyle": "italic"
      },
      "scope": [
        // "variable.parameter",
        "entity.other.attribute-name",
        "entity.name.tag",
        "entity.name.function.macro.rust",

        "markup.italic",
        "emphasis"
      ]
    },

    {
      "name": "underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },

    {
      "name": "underline bold",
      "scope": [
        "markup.bold markup.underline",

        "markup.underline entity.name.function.declaration",
        "markup.underline meta.definition entity.name.function",
        "markup.underline meta.function.definition entity.name.function",
        "entity.name.function.declaration markup.underline",
        "meta.definition entity.name.function markup.underline",
        "meta.function.definition entity.name.function markup.underline"
      ],
      "settings": {
        "fontStyle": "bold underline"
      }
    },

    {
      "name": "noise",
      "settings": {
        "foreground": "#48587c"
      },
      "scope": [
        "meta.brace",
        "meta.bracket",
        "punctuation.brackets",
        "punctuation.section",
        "punctuation.definition",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.colon",
        "punctuation.operation",
        "punctuation.semi",
        "punctuation.comma"
      ]
    },

    // === some old colors ===

    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    }
  ]
}
